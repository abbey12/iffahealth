{
  "name": "iffahealth-enhanced-api",
  "version": "2.0.0",
  "description": "IFFAHEALTH Enhanced API - Production-ready telehealth platform backend",
  "main": "enhanced_server.js",
  "scripts": {
    "start": "node enhanced_server.js",
    "dev": "nodemon enhanced_server.js",
    "setup-db": "node scripts/setup_enhanced_db.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build": "echo 'No build step required for Node.js'",
    "docker:build": "docker build -t iffahealth-api .",
    "docker:run": "docker run -p 3000:3000 iffahealth-api",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "health": "curl http://localhost:3000/health",
    "docs": "curl http://localhost:3000/api/docs"
  },
  "keywords": [
    "telehealth",
    "healthcare",
    "api",
    "nodejs",
    "express",
    "postgresql",
    "jwt",
    "mobile",
    "react-native",
    "medical",
    "appointments",
    "prescriptions",
    "lab-tests",
    "medical-records",
    "payouts",
    "notifications"
  ],
  "author": "IFFAHEALTH Team",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.0.1",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "nodemailer": "^6.9.7",
    "twilio": "^4.19.0",
    "stripe": "^14.7.0",
    "agora-access-token": "^2.0.4",
    "axios": "^1.6.2",
    "compression": "^1.7.4",
    "express-json-validator-middleware": "^4.1.0",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "express-winston": "^4.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "redis": "^4.6.10",
    "bull": "^4.12.2",
    "socket.io": "^4.7.4",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "express-fileupload": "^1.4.3",
    "sharp": "^0.32.6",
    "pdf-lib": "^1.17.1",
    "qrcode": "^1.5.3",
    "node-cron": "^3.0.3",
    "nodemailer-express-handlebars": "^5.0.0",
    "handlebars": "^4.7.8",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.3.0",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.43",
    "crypto": "^1.0.1",
    "fs-extra": "^11.1.1",
    "path": "^0.12.7",
    "url": "^0.11.3",
    "querystring": "^0.2.1",
    "util": "^0.12.5",
    "events": "^3.3.0",
    "stream": "^0.0.2",
    "buffer": "^6.0.3",
    "process": "^0.11.10"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/pg": "^8.10.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.7",
    "@types/compression": "^1.7.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/lodash": "^4.14.202",
    "@types/fs-extra": "^11.0.4",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "nyc": "^15.1.0",
    "coveralls": "^3.1.1",
    "faker": "^6.6.6",
    "factory-girl": "^5.0.4",
    "chai": "^4.3.10",
    "mocha": "^10.2.0",
    "sinon": "^17.0.1",
    "nock": "^13.4.0",
    "timekeeper": "^2.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/scripts/**",
      "!**/test/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iffahealth/api.git"
  },
  "bugs": {
    "url": "https://github.com/iffahealth/api/issues"
  },
  "homepage": "https://github.com/iffahealth/api#readme",
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/iffahealth"
  }
}
